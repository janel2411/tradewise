{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      },
      {
        "src": "trades/requirements.txt",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "frontend/build/static/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "trades/$1"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/build/$1"
      }
    ]
  }